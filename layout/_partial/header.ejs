<header id="header">
  <a href="<%- url_for("/") %>"><div id="logo" style="background-image: url(<%- url_for(theme.logo.url) %>);"></div>
  <div id="title">
      <h1><%= config.title %></h1>
    </div></a>
  <div id="nav">
    <ul>
      <% for (var i in theme.nav) { %>
        <li><a href="<%- url_for(theme.nav[i]) %>"><%= __('nav.'+i).replace("nav.", "") %></a></li>
      <% } %>
    </ul>
  </div>
</header>